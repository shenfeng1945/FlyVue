    "sass-loader": "^7.1.0",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0",
    "vue-highlightjs": "^1.3.3",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "1.0.2"
